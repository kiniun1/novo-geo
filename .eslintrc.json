{
    "env": {
        "browser": true,
        "amd": true,
        "node": true,
        "es2021": true,
        "jest":true
    },
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-undef": "off",
        "prefer-const": "off",
        "jest/no-process-env": "off"
    },
    "globals": {
        "process": true
    },
    "plugins": ["jest"]

}
